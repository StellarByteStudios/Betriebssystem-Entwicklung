ENTRY(main)

SECTIONS {
    . = 0x10000000000;   /* load at address 1TB */

    ___KERNEL_DATA_START__ = .;

  .boot :
    {
        /* ensure that the multiboot header is at the beginning */
        *(_multiboot_header)
    }

    .text :
    {
        *(.main*)
        *(.text*)
    }

   .bss : 
    {
      ___BSS_START__ = .;
      *(".bss")
      *(".bss.*")
      ___BSS_END__ = .;
    }

    ___KERNEL_DATA_END__ = .;

}
